{
  "name": "help-chat",
  "version": "2.0.0",
  "description": "A minimal Electron application",
  "keywords": [
    "web-browser",
    "material",
    "electron",
    "react",
    "mobx",
    "styled-components"
  ],
  "main": "build/main.js",
  "author": {
    "name": "Yevhenii SHulha",
    "email": "y.shulga393@gmail.com"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV='dev' NODE_TLS_REJECT_UNAUTHORIZED=0 node fuse.js",
    "build": "node fuse.js",
    "start": "cross-env ENV='dev' electron .",
    "compile-win32": "npm run build && electron-builder -w -p always",
    "compile-darwin": "npm run build && electron-builder -m -p always",
    "compile-linux": "npm run build && electron-builder -l -p always",
    "lint": "tslint \"src/**/*.ts*\"",
    "lint-fix": "prettier --write \"src/**/*.ts*\" && tslint \"src/**/*.ts*\" --fix",
    "postinstall": "electron-builder install-app-deps"
  },
  "pre-commit": [
    "lint-fix"
  ],
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/chrome": "0.0.83",
    "@types/gsap": "1.20.2",
    "@types/leveldown": "4.0.0",
    "@types/levelup": "3.1.0",
    "@types/nedb": "1.8.7",
    "@types/node": "12.0.0",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
    "@types/styled-components": "4.1.14",
    "concurrently": "4.1.0",
    "cross-env": "5.2.0",
    "electron": "5.0.1",
    "electron-builder": "^23.6.0",
    "fuse-box": "3.7.1",
    "prettier": "1.17.0",
    "terser": "3.17.0",
    "tslint": "5.16.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.3.3",
    "typescript-plugin-styled-components": "1.4.2"
  },
  "dependencies": {
    "@cliqz/adblocker": "0.9.1",
    "@electron/rebuild": "^3.2.10",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "axios": "^0.18.1",
    "braces": "^3.0.2",
    "electron-extensions": "1.0.0",
    "electron-fetch": "^1.9.1",
    "electron-log": "3.0.5",
    "electron-updater": "4.0.6",
    "file-type": "11.0.0",
    "gsap": "2.1.2",
    "icojs": "0.12.3",
    "leveldown": "5.0.3",
    "levelup": "4.0.1",
    "lodash": "^4.17.15",
    "mobx": "5.9.4",
    "mobx-react": "5.4.3",
    "mouse-hooks": "0.3.1",
    "mui": "^0.0.1",
    "nedb": "1.8.0",
    "node-fetch": "^3.3.0",
    "node-vibrant": "3.2.0-alpha",
    "node-window-manager": "0.8.4",
    "pretty-bytes": "5.2.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-scripts": "^5.0.1",
    "styled-components": "4.2.0",
    "tldts": "4.0.6",
    "ws": "^7.1.1"
  }
}
